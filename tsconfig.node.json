{
    "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
    "include": [
        "electron.vite.config.*",
        "src/main/*",
        "src/preload/*",
        "src/constants/*",
        "src/services/*",
        "src/models/*",
        "src/models/enums/*",
        "src/renderer/src/Routes.ts",
    ],
    "compilerOptions": {
        "composite": true,
        "types": [
            "electron-vite/node"
        ]
    }
}
